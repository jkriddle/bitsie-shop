@using System.Web.Optimization
@using Bitsie.Shop.Domain
@model Bitsie.Shop.Web.Models.BaseViewModel

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta name="robots" content="noindex, nofollow" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="/favicon.ico">
    <meta name="description" content="">
    <meta name="author" content="">
      
    @Styles.Render("~/Bundles/Styles/core")
    @Styles.Render("~/Bundles/Styles/plugins")
    @Styles.Render("~/Bundles/Styles/app")
    @Styles.Render("~/Bundles/Styles/public")
    @Styles.Render("~/Bundles/Styles/checkout")

      
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
    <link href="/Content/css/app/mobile.less"  rel="stylesheet" type="text/css" media="only screen and (max-device-width: 480px)">
      <link href='//fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
      <script src="//html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
      
    @Scripts.Render("~/Bundles/Scripts/core")
    @Scripts.Render("~/Bundles/Scripts/plugins")
    @Scripts.Render("~/Bundles/Scripts/app")
    @Scripts.Render("~/Bundles/Scripts/public")
      
    @RenderSection("scripts", required: false)
  </head>

  <body class="merchant">
        @Html.AntiForgeryToken()
        <div id="wrap">

		    <div id="content" class="last">
                @RenderBody()
            </div>

            <div class="clear"></div>

        </div>
      
        <span class="logo print" style="width:200px;">Bitsie Shop<br />http://shop.bitsie.com</span>

        <a class="logo noprint" href="http://shop.bitsie.com">
            <img alt="" src="data:image/png;base64," alt="Bitsie Shop"/>
        </a>
       

  </body>
</html>
